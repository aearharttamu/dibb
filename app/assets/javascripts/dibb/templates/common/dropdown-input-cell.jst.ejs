<td>
  <select type="text" class="form-control" id="<%= field_name %>" placeholder="<%= field_title %>">
  	<option></option>
  	<% for (var i = 0; i < options.length; i++) { %>
  	<option value="<%= options[i].value %>" <%= (options[i].value === field_value) ? 'selected="selected"' : ""  %>><%= options[i].text %></option>
  	<% } %>
  </select>
</td>

